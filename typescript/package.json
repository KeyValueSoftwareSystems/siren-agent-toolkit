{
  "name": "@trysiren/agent-toolkit",
  "version": "0.1.1",
  "description": "Agent toolkit for Siren notification platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./openai": {
      "import": "./dist/openai/index.js",
      "require": "./dist/openai/index.js",
      "types": "./dist/openai/index.d.ts"
    },
    "./langchain": {
      "import": "./dist/langchain/index.js",
      "require": "./dist/langchain/index.js",
      "types": "./dist/langchain/index.d.ts"
    },
    "./ai-sdk": {
      "import": "./dist/ai-sdk/index.js",
      "require": "./dist/ai-sdk/index.js",
      "types": "./dist/ai-sdk/index.d.ts"
    },
    "./mcp-server": {
      "import": "./dist/mcp-server/index.js",
      "require": "./dist/mcp-server/index.js",
      "types": "./dist/mcp-server/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run type-check && npm run test && npm run build",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "siren",
    "notifications",
    "agent",
    "toolkit",
    "ai",
    "openai",
    "langchain",
    "ai-sdk",
    "mcp-server"
  ],
  "workspaces": [
    ".",
    "examples/*"
  ],
  "author": "Siren",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.13.1",
    "@trysiren/node": "^0.1.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "peerDependencies": {
    "@langchain/core": "^0.1.0",
    "ai": "^4.0.0",
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@langchain/core": {
      "optional": true
    },
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.19.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "VERSION",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KeyValueSoftwareSystems/siren-agent-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/KeyValueSoftwareSystems/siren-agent-toolkit/issues"
  },
  "homepage": "https://github.com/KeyValueSoftwareSystems/siren-agent-toolkit#readme"
}
